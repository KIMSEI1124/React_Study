{
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/no-unresolved": [
            2,
            {
                "caseSensitive": false
            }
        ],
        "object-curly-newline": [
            "error",
            {
                "ObjectExpression": {
                    "consistent": true,
                    "multiline": true
                },
                "ObjectPattern": {
                    "consistent": true,
                    "multiline": true
                },
                "ImportDeclaration": "never",
                "ExportDeclaration": {
                    "multiline": true,
                    "minProperties": 3
                }
            }
        ]
    }
}